{
  "name": "kerluc/filemanager-bundle",
  "type": "symfony-bundle",
  "description": "FileManager is a simple Multilingual File Manager Bundle for Symfony (fork)",
  "keywords": [
    "symfony",
    "bundle",
    "file manager"
  ],
  "homepage": "https://github.com/artgris/FileManagerBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Arthur Gribet",
      "email": "a.gribet@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "symfony/translation": "^4.0",
    "symfony/form": "^4.0",
    "symfony/validator": "^4.0",
    "symfony/twig-bundle": "^4.0",
    "symfony/twig-bridge": "^4.0",
    "symfony/asset": "^4.0",
    "twig/twig": "^2.7 || ^3.0",
    "oomphinc/composer-installers-extender": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0|^5.0",
    "symfony/security-bundle": "^4.0",
    "symfony/css-selector": "^4.0",
    "symfony/browser-kit": "^4.0",
    "symfony/var-dumper": "^4.0",
    "symfony/yaml": "~2.3|~3.0|^4.0",
    "symfony/templating":  "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Artgris\\Bundle\\FileManagerBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Artgris\\Bundle\\FileManagerBundle\\Tests\\": "/Tests/"
    }
  }
}
